      <add name="connectionTimeout" value="360000" />
      <add name="requestRetries" value="1" />
      <!--<add name="clientId" value="AUASNhD7YM7dc5Wmc5YE9pEsC0o4eVOyYWO9ezXWBu2XTc63d3Au_s9c-v-U"/>
