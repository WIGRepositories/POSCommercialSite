  <!--Configure logging using the log4net library-->
  <log4net>
    <appender name="FileAppender" type="log4net.Appender.FileAppender">
      <file value="D:\F\WIP\lampex\2016\BTPOSDashboard\log\PayPal.SDK.Sample.log" />
      <appendToFile value="true" />
      <layout type="log4net.Layout.PatternLayout">
        <conversionPattern value="%date [%thread] %-5level %logger [%property{NDC}] %message%newline" />
      </layout>
    </appender>
    <root>
      <level value="DEBUG" />
      <appender-ref ref="FileAppender" />
    </root>
  </log4net>

  <appSettings>
    <add key="PayPalLogger" value="PayPal.Log.Log4netLogger" />
  </appSettings>
  
  <connectionStrings>
    <add name="btposdb" connectionString="Data Source=localhost;Initial Catalog=POSDashboard;integrated security=sspi;" />
  </connectionStrings>
  <system.web>
    <compilation debug="true" targetFramework="4.5" />
    <httpRuntime targetFramework="4.5" />
  </system.web>
  
  <runtime>
    <assemblyBinding xmlns="urn:schemas-microsoft-com:asm.v1">
